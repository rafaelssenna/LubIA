{
  "name": "@lubia/api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "prisma generate && tsup src/index.ts --format cjs,esm --dts",
    "start": "node dist/index.js",
    "lint": "eslint src/",
    "db:migrate": "prisma migrate dev",

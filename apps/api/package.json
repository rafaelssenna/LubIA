    "db:studio": "prisma studio"
  },
  "dependencies": {
    "@fastify/cors": "^10.0.1",
    "@lubia/types": "workspace:*",
    "@prisma/client": "^6.6.0",
    "dotenv": "^17.2.3",
    "fastify": "^5.2.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^20",
    "prisma": "^6.6.0",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5"
  }
}
